<div id="categories">
  <ul class="nav nav-tabs">
    <% current_categories.each do |category| %>
    <li class="<%= 'active' if params[:id].to_i == category.id %>">
      <%= link_to category.name, category_path(category) %>
    </li>
    <% end %>
  <li><%= link_to "Menu", items_path %></li>
  </ul>
</div>
